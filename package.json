{
  "name": "generate-assets",
  "version": "0.0.0",
  "license": "MIT",
  "bin": "dist/cli.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "tsc",
    "start": "npm run build && dist/cli.js",
    "start:debug": "DEV=true npm run build && dist/cli.js",
    "format": "prettier -uw --ignore-path .gitignore .",
    "lint": "eslint . --cache --fix --ignore-path .gitignore",
    "prepare": "husky install"
  },
  "files": [
    "dist/cli.js"
  ],
  "dependencies": {
    "ink": "^3.2.0",
    "meow": "^9.0.0",
    "react": "^17.0.2",
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "@sindresorhus/tsconfig": "^2.0.0",
    "@types/react": "^17.0.35",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-airbnb-typescript": "14.0.2",
    "eslint-config-xo-react": "^0.25.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-react": "^7.27.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unicorn": "36.0.0",
    "eslint-plugin-react-prefer-function-component": "^0.0.7",
    "husky": "^7.0.4",
    "lint-staged": "^12.0.3",
    "prettier": "2.4.1",
    "react-devtools": "^4.21.0",
    "typescript": "4.4.4"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown",
    "*.{js,ts,tsx}": "eslint --cache --fix"
  }
}
